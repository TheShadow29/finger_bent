I = imread('3_fingers.jpg');
J = rgb2gray(I);

background = imopen(J,strel('disk',15));
I2 = J + background;

I3 = imadjust(I2);

level = graythresh(I3);
bw = im2bw(I3,level);
wb=imcomplement(bw);
bw = bwareaopen(bw, 500000);
wb = bwareaopen(wb, 50000);

%figure;imshow(bw);


figure;imshow(wb);
cc=bwconncomp(wb);
graind
